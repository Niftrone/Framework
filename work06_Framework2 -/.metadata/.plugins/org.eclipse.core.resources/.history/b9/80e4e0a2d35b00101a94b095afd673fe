<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
												 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
     <!--
     	ㅇ Hello instance 생성 서술적, 선언적 기술
			1. bean element  
				- id : instance identifier 
				- class : instance 생성 Bean (FQCN)
				
	player01 이름으로 빈을 생성 (Player02) | DiceAImple주입 | 생성자 -->
     -->
     <bean id="diceA" class="spring.service.dice.impl.DiceAImple"></bean>
     <bean id="diceB" class="spring.service.dice.impl.DiceBImple"></bean>
     <bean id="diceC" class="spring.service.dice.impl.DiceAImple"></bean>
     <bean id="player02" class="spring.service.dice.impl.DiceAImple"></bean>
	
</beans>